# Specifies the AWS Region to send requests to.
AWS_DEFAULT_REGION=us-west-1

# Specifies the AWS access key associated with an IAM user or role.
AWS_ACCESS_KEY_ID=your-aws-access-key-id

# Specifies the AWS secret key associated with the AWS access key.
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key

# Specifies the S3 bucket to queue unprocessed frames in.
GRASSLAND_FRAME_S3_BUCKET=your-grassland-frame-s3-bucket

# Specifies the S3 bucket to store model data in.
GRASSLAND_MODEL_S3_BUCKET=your-grassland-model-s3-bucket

# Specifies the Mapbox access token for API use.
MapboxAccessToken=your-mapbox-access-token
